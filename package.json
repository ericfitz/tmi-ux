{
  "name": "tmi-ux",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:test": "ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev": "ng serve --open",
    "dev:staging": "ng serve --configuration=staging --open",
    "dev:test": "ng serve --configuration=test --open",
    "dev:prod": "ng serve --configuration=production --open",
    "dev:custom": "ng serve --host=${TMI_INTERFACE:-0.0.0.0} --port=${TMI_PORT:-4200} --ssl=${TMI_SSL:-false} --ssl-key=${TMI_SSL_KEY:-} --ssl-cert=${TMI_SSL_CERT:-} --open",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:all": "pnpm run lint && pnpm run lint:scss",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json,md}\"",
    "check": "pnpm run format:check && pnpm run lint:all",
    "prepare": "husky",
    "optimize-images": "node convert-images.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.2.8",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/material": "^19.2.8",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@antv/x6": "^2.18.1",
    "@antv/x6-angular-shape": "^2.0.2",
    "@antv/x6-plugin-clipboard": "^2.1.6",
    "@antv/x6-plugin-dnd": "^2.1.1",
    "@antv/x6-plugin-export": "^2.1.6",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-keyboard": "^2.2.3",
    "@antv/x6-plugin-scroller": "^2.0.10",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@jsverse/transloco": "^7.6.1",
    "@types/file-saver": "^2.0.7",
    "file-saver-es": "^2.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.5",
    "@angular-eslint/builder": "^19.3.0",
    "@angular-eslint/eslint-plugin": "^19.3.0",
    "@angular-eslint/eslint-plugin-template": "^19.3.0",
    "@angular-eslint/template-parser": "^19.3.0",
    "@angular/cli": "^19.2.5",
    "@angular/compiler-cli": "^19.2.0",
    "@eslint/js": "^9.24.0",
    "@rollup/rollup-darwin-arm64": "^4.40.0",
    "@types/jasmine": "~5.1.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "globals": "^16.0.0",
    "http-proxy-middleware": "^3.0.5",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.5.0",
    "lmdb": "^3.2.6",
    "prettier": "^3.5.3",
    "stylelint": "^16.18.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "resolutions": {
    "http-proxy-middleware": "3.0.5"
  },
  "pnpm": {
    "overrides": {
      "vite@>=6.2.0 <=6.2.6": ">=6.2.7",
      "glob@<9.0.0": "^9.3.5",
      "rimraf@<4.0.0": "^4.4.1",
      "inflight@*": "^1.0.6"
    }
  }
}
