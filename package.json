{
  "name": "tmi-ux",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:test": "ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:component": "vitest run \"src/app/pages/tm/tm.component.spec.ts\"",
    "test:e2e": "node scripts/run-cypress-tests.js",
    "test:e2e:open": "node scripts/run-cypress-tests.js --open",
    "test:e2e:component": "node scripts/run-cypress-tests.js --component",
    "test:e2e:component:open": "node scripts/run-cypress-tests.js --component --open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "ng serve --open",
    "dev:staging": "ng serve --configuration=staging --open",
    "dev:test": "ng serve --configuration=test --open",
    "dev:prod": "ng serve --configuration=production --open",
    "dev:local": "ng serve --configuration=local --open",
    "dev:custom": "ng serve --host=${TMI_INTERFACE:-0.0.0.0} --port=${TMI_PORT:-4200} --ssl=${TMI_SSL:-false} --ssl-key=${TMI_SSL_KEY:-} --ssl-cert=${TMI_SSL_CERT:-} --open",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:all": "pnpm run lint && pnpm run lint:scss",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json,md}\"",
    "check": "pnpm run format:check && pnpm run lint:all",
    "bump": "pnpm update && pnpm install",
    "prepare": "husky",
    "optimize-images": "node convert-images.js"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular/animations": "^20.0.5",
    "@angular/cdk": "^20.0.4",
    "@angular/common": "^20.0.5",
    "@angular/compiler": "^20.0.5",
    "@angular/core": "^20.0.5",
    "@angular/forms": "^20.0.5",
    "@angular/material": "^20.0.4",
    "@angular/platform-browser": "^20.0.5",
    "@angular/platform-browser-dynamic": "^20.0.5",
    "@angular/router": "^20.0.5",
    "@antv/x6": "^2.18.1",
    "@antv/x6-plugin-export": "^2.1.6",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@jsverse/transloco": "^7.6.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.19.0",
    "@angular-devkit/build-angular": "^20.0.4",
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/eslint-plugin": "^20.1.1",
    "@angular-eslint/eslint-plugin-template": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular-eslint/template-parser": "^20.1.1",
    "@angular/cli": "^20.0.5",
    "@angular/compiler-cli": "^20.0.5",
    "@eslint/js": "^9.30.0",
    "@rollup/rollup-darwin-arm64": "^4.44.2",
    "@testing-library/angular": "^17.4.0",
    "@types/cypress-image-snapshot": "^3.1.9",
    "@types/jasmine": "~5.1.8",
    "@types/jsdom": "^21.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "angular-eslint": "^20.1.1",
    "cypress": "^14.5.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "globals": "^16.2.0",
    "http-proxy-middleware": "3.0.5",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "stylelint": "^16.21.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "vitest": "^3.2.4",
    "webpack-bundle-analyzer": "^4.10.2"
  },
  "packageManager": "pnpm@10.12.1",
  "resolutions": {
    "http-proxy-middleware": "3.0.5"
  },
  "pnpm": {
    "overrides": {
      "http-proxy-middleware": "3.0.5",
      "vite@>=6.2.0 <=6.2.6": ">=6.2.7",
      "glob@<9.0.0": "^9.3.5",
      "rimraf@<4.0.0": "^4.4.1",
      "inflight@*": "^1.0.6",
      "esbuild@<0.25.0": "^0.25.4",
      "happy-dom@<15.10.2": "^15.10.2",
      "cypress-image-snapshot>cypress": "^14.5.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "minimatch@<9.0.0": "^9.0.5"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "jest"
      ],
      "allowedVersions": {
        "cypress": "14.3.3",
        "jest": "30.0.2",
        "cypress-image-snapshot>cypress": "14.3.3",
        "@angular-devkit/build-angular>jest": "30.0.2",
        "jest-image-snapshot>jest": "30.0.2"
      }
    },
    "onlyBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
