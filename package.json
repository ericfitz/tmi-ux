{
  "name": "tmi-ux",
  "version": "0.7.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:test": "ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:tm": "vitest run \"src/app/pages/tm/tm.component.spec.ts\"",
    "test:dfd": "vitest run \"src/app/pages/dfd/dfd.component.spec.ts\"",
    "test:dfdinteg": "node scripts/cypress-clean.js --spec=\"cypress/e2e/dfd-*.cy.ts\"",
    "test:e2e": "node scripts/cypress-clean.js",
    "test:e2e:open": "node scripts/cypress-clean.js --open",
    "test:e2e:component": "node scripts/cypress-clean.js --component",
    "test:e2e:component:open": "node scripts/cypress-clean.js --component --open",
    "test:e2e:clean": "node scripts/cypress-clean.js",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "ng serve --open",
    "dev:staging": "ng serve --configuration=staging --open",
    "dev:test": "ng serve --configuration=test --open",
    "dev:prod": "ng serve --configuration=production --open",
    "dev:local": "ng serve --configuration=local --open",
    "dev:custom": "ng serve --host=${TMI_INTERFACE:-0.0.0.0} --port=${TMI_PORT:-4200} --ssl=${TMI_SSL:-false} --ssl-key=${TMI_SSL_KEY:-} --ssl-cert=${TMI_SSL_CERT:-} --open",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:all": "pnpm run lint && pnpm run lint:scss",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json,md}\"",
    "check": "pnpm run format:check && pnpm run lint:all",
    "validate-json": "node scripts/validate-json.cjs",
    "validate-json:i18n": "node scripts/validate-json.cjs \"src/assets/i18n/*.json\"",
    "validate-json:all": "node scripts/validate-json.cjs \"**/*.json\" --strict",
    "validate-jsonc": "node scripts/validate-json.cjs --jsonc",
    "validate-jsonc:tsconfig": "node scripts/validate-json.cjs \"tsconfig*.json\" --jsonc",
    "unused-i18n": "node scripts/check-unused-i18n-keys.cjs src/assets/i18n/en-US.json",
    "validate-all": "pnpm run validate-json:i18n && pnpm run validate-jsonc:tsconfig && pnpm run unused-i18n",
    "sort-json": "uv run scripts/sort-json.py -y",
    "compare-json": "uv run scripts/sort-json.py -y",
    "check-i18n": "python3 scripts/sort-json.py src/assets/i18n/en-US.json --diff src/assets/i18n/* -y",
    "check-i18n:dry-run": "python3 scripts/sort-json.py src/assets/i18n/en-US.json --diff src/assets/i18n/* -y --dry-run",
    "loc-dedupe": "uv run scripts/find_duplicate_localizations.py --skippolicy",
    "bump": "pnpm update --verbose && pnpm install --verbose",
    "update-shared": "git subtree pull --prefix=shared-api https://github.com/ericfitz/tmi.git shared --squash",
    "check:security": "tsx scripts/check-security.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/cdk": "^20.1.6",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/material": "^20.1.6",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.1.7",
    "@angular/router": "^20.1.7",
    "@antv/x6": "^2.18.1",
    "@antv/x6-plugin-export": "^2.1.6",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@jsverse/transloco": "^7.6.1",
    "jspdf": "^3.0.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.20.1",
    "@angular-devkit/build-angular": "^20.2.1",
    "@angular-eslint/builder": "^20.2.0",
    "@angular-eslint/eslint-plugin": "^20.2.0",
    "@angular-eslint/eslint-plugin-template": "^20.2.0",
    "@angular-eslint/schematics": "^20.2.0",
    "@angular-eslint/template-parser": "^20.2.0",
    "@angular/cli": "^20.2.1",
    "@angular/compiler-cli": "^20.1.7",
    "@eslint/js": "^9.34.0",
    "@testing-library/angular": "^18.0.0",
    "@types/cypress-image-snapshot": "^3.1.9",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "angular-eslint": "^20.2.0",
    "cypress": "^14.5.4",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.2.0",
    "glob": "^11.0.3",
    "globals": "^16.3.0",
    "http-proxy-middleware": "3.0.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.23.1",
    "stylelint-config-standard-scss": "^15.0.1",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.42.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.12.1",
  "resolutions": {
    "http-proxy-middleware": "3.0.5"
  },
  "pnpm": {
    "overrides": {
      "http-proxy-middleware": "3.0.5",
      "vite@>=6.2.0 <=6.2.6": ">=6.2.7",
      "glob@<9.0.0": "^9.3.5",
      "rimraf@<4.0.0": "^4.4.1",
      "inflight@*": "^1.0.6",
      "esbuild@<0.25.0": "^0.25.4",
      "happy-dom@<15.10.2": "^15.10.2",
      "cypress-image-snapshot>cypress": "^14.5.0",
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "minimatch@<9.0.0": "^9.0.5",
      "on-headers@<1.1.0": ">=1.1.0",
      "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
      "@eslint/plugin-kit@<0.3.4": ">=0.3.4"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "jest",
        "@jest/types",
        "jest-diff"
      ],
      "allowedVersions": {
        "cypress": "14.3.3",
        "cypress-image-snapshot>cypress": "14.3.3"
      }
    },
    "onlyBuiltDependencies": [
      "unrs-resolver"
    ]
  }
}
