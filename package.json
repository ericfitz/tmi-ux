{
  "name": "tmi-ux",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:staging": "ng build --configuration=staging",
    "build:test": "ng build --configuration=test",
    "watch": "ng build --watch --configuration development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:component": "vitest run \"src/app/pages/tm/tm.component.spec.ts\"",
    "test:e2e": "node scripts/run-cypress-tests.js",
    "test:e2e:open": "node scripts/run-cypress-tests.js --open",
    "test:e2e:component": "node scripts/run-cypress-tests.js --component",
    "test:e2e:component:open": "node scripts/run-cypress-tests.js --component --open",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "ng serve --open",
    "dev:staging": "ng serve --configuration=staging --open",
    "dev:test": "ng serve --configuration=test --open",
    "dev:prod": "ng serve --configuration=production --open",
    "dev:local": "ng serve --configuration=local --open",
    "dev:custom": "ng serve --host=${TMI_INTERFACE:-0.0.0.0} --port=${TMI_PORT:-4200} --ssl=${TMI_SSL:-false} --ssl-key=${TMI_SSL_KEY:-} --ssl-cert=${TMI_SSL_CERT:-} --open",
    "lint": "eslint \"src/**/*.{ts,html}\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "lint:all": "pnpm run lint && pnpm run lint:scss",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json,md}\"",
    "check": "pnpm run format:check && pnpm run lint:all",
    "bump": "pnpm update && pnpm install",
    "prepare": "husky",
    "optimize-images": "node convert-images.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.3",
    "@angular/cdk": "^20.0.3",
    "@angular/common": "^20.0.3",
    "@angular/compiler": "^20.0.3",
    "@angular/core": "^20.0.3",
    "@angular/forms": "^20.0.3",
    "@angular/material": "^20.0.3",
    "@angular/platform-browser": "^20.0.3",
    "@angular/platform-browser-dynamic": "^20.0.3",
    "@angular/router": "^20.0.3",
    "@antv/x6": "^2.18.1",
    "@antv/x6-angular-shape": "^2.0.2",
    "@antv/x6-plugin-clipboard": "^2.1.6",
    "@antv/x6-plugin-dnd": "^2.1.1",
    "@antv/x6-plugin-export": "^2.1.6",
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-keyboard": "^2.2.3",
    "@antv/x6-plugin-scroller": "^2.0.10",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-plugin-transform": "^2.1.8",
    "@jsverse/transloco": "^7.6.1",
    "@types/file-saver": "^2.0.7",
    "file-saver-es": "^2.0.5",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.17.1",
    "@angular-devkit/build-angular": "^20.0.2",
    "@angular-eslint/builder": "^19.8.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "19.4.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^20.0.3",
    "@eslint/js": "^9.28.0",
    "@rollup/rollup-darwin-arm64": "^4.43.0",
    "@testing-library/angular": "^17.4.0",
    "@types/cypress-image-snapshot": "^3.1.9",
    "@types/jasmine": "~5.1.8",
    "@types/jsdom": "^21.1.7",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitest/coverage-v8": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "angular-eslint": "19.4.0",
    "cypress": "^14.4.1",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "globals": "^16.2.0",
    "http-proxy-middleware": "3.0.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.2",
    "lmdb": "^3.4.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.20.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.0",
    "vitest": "^3.2.3"
  },
  "packageManager": "pnpm@10.7.1+sha512.2d92c86b7928dc8284f53494fb4201f983da65f0fb4f0d40baafa5cf628fa31dae3e5968f12466f17df7e97310e30f343a648baea1b9b350685dafafffdf5808",
  "resolutions": {
    "http-proxy-middleware": "3.0.5"
  },
  "pnpm": {
    "overrides": {
      "vite@>=6.2.0 <=6.2.6": ">=6.2.7",
      "glob@<9.0.0": "^9.3.5",
      "rimraf@<4.0.0": "^4.4.1",
      "inflight@*": "^1.0.6",
      "esbuild@<0.25.0": "^0.25.4",
      "happy-dom@<15.10.2": "^15.10.2",
      "cypress-image-snapshot>cypress": "$cypress"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "jest"
      ],
      "allowedVersions": {
        "cypress": "14.3.3",
        "jest": "29.7.0",
        "cypress-image-snapshot>cypress": "14.3.3"
      }
    }
  }
}
