  "edgeStyle": {
    "router": "manhattan",
    "connector": {
      "name": "rounded",
      "args": {
        "radius": 8
      }
    },
    "attrs": {
      "line": {
        "stroke": "#5F95FF",
        "strokeWidth": 1,
        "targetMarker": {
          "name": "classic",
          "size": 8
        }
      }
    },
    "zIndex": 0
  },
  "cssStyles": [
    {
      "selector": ".diagram-editor-canvas",
      "properties": {
        "background-color": "#f5f5f5",
        "border": "1px solid #e0e0e0"
      }
    },
    {
      "selector": ".diagram-editor-palette",
      "properties": {
        "background-color": "#ffffff",
        "border-right": "1px solid #e0e0e0"
      }
    },
    {
      "selector": ".diagram-editor-properties",
      "properties": {
        "background-color": "#ffffff",
        "border-left": "1px solid #e0e0e0"
      }
    }
  ],
  "graphOptions": {
    "grid": true,
    "background": {
      "color": "#f5f5f5"
    },
    "connecting": {
      "anchor": "center",
      "connectionPoint": "boundary",
      "allowBlank": false,
      "snap": {
        "radius": 20
      }
    }
  }
}
