    "shadow": true,
    "rounded": true,
    "jettySize": "auto",
    "orthogonalLoop": 1
  },
  "styles": {
    "highlighted": {
      "strokeColor": "#ff0000",
      "strokeWidth": 3,
      "fillColor": "#ffffff",
      "rounded": true,
      "shadow": true
    },
    "edgeCreation": {
      "strokeColor": "#4caf50",
      "strokeWidth": 2,
      "fillColor": "#e8f5e9",
      "rounded": true,
      "shadow": true
    },
    "process": {
      "fillColor": "#f5f5f5",
      "strokeColor": "#666666",
      "rounded": true,
      "shadow": true,
      "whiteSpace": "wrap",
      "html": 1
    },
    "store": {
      "shape": "cylinder",
      "whiteSpace": "wrap",
      "html": 1,
      "boundedLbl": 1,
      "fillColor": "#dae8fc",
      "strokeColor": "#6c8ebf",
