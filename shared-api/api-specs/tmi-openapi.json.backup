                    }
                  },
                  "invalid_token": {
                    "summary": "Invalid refresh token",
                    "value": {
                      "error": "Invalid refresh token",
                      "error_description": "The provided refresh token is not valid"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Error"
                }
              }
            }
          }
        }
      }
